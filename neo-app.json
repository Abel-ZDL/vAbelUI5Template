{
	"welcomeFile": "/webapp/index.html",
	"routes": [{
		"path": "/resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/resources"
		},
		"description": "SAPUI5 Resources"
	}, {
		"path": "/test-resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/test-resources"
		},
		"description": "SAPUI5 Test Resources"
	}, {
		"path": "/destinations/northwind",
		"target": {
			"type": "destination",
			"name": "northwind"
		},
		"description": "Northwind OData Service"
	}, {
		"path": "/destinations/ems_backend_service",
		"target": {
			"type": "destination",
			"name": "ems_backend_service"
		},
		"description": "EMS Backend Service on CF"
	}],
	"sendWelcomeFileRedirect": true,
	"cacheControl": [{
		"directive": "private",
		"maxAge": 0,
		"path": "**.html"
	}, {
		"directive": "private",
		"maxAge": 0,
		"path": "sap-ui-cachebuster-info.json"
	}, {
		"directive": "public",
		"maxAge": 31536000
	}]
}